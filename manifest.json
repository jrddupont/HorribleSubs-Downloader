{
	"name": "HorribleSubs Downloader",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Opens the magnet link for all of a specified resolution.",
	"icons": {
		"128": "icons/hsd32.png",
		"128": "icons/hsd64.png",
		"128": "icons/hsd128.png"
		},
	"permissions": ["activeTab", "tabs"],
	"background": {
		"scripts": ["background.js"]
	},
	"page_action": {
		"default_icon": "icons/hsd32.png",
		"default_title": "Open magnets on this page"
	}
}
